{
  "extends": ["prettier"],
  "parser": "babel-eslint",
  "plugins": ["react-hooks"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "jest": true
  },
  "rules": {
    "prefer-const": "error",
    "react-hooks/rules-of-hooks": "error"
  }
}
